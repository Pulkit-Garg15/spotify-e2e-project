{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotify-e2e-factory"
		},
		"AzureDataLakeStorage01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage01'"
		},
		"AzureSqlDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDB'"
		},
		"AzureDataLakeStorage01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifye2edev.dfs.core.windows.net/"
		},
		"AzureSqlDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azure-sql15.database.windows.net"
		},
		"AzureSqlDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotify_src_db"
		},
		"AzureSqlDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pulkitgarg1501"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotify-e2e-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage01_accountKey": {
						"value": "[parameters('AzureDataLakeStorage01_accountKey')]"
					},
					"AzureSqlDB_password": {
						"value": "[parameters('AzureSqlDB_password')]"
					},
					"AzureDataLakeStorage01_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage01_properties_typeProperties_url')]"
					},
					"AzureSqlDB_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDB_properties_typeProperties_server')]"
					},
					"AzureSqlDB_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDB_properties_typeProperties_database')]"
					},
					"AzureSqlDB_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}